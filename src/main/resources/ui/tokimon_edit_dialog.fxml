<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca.cmpt213.asn5_1.ui.TokimonEditDialogController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="ID:"/>
        <TextField fx:id="idField" layoutX="100.0" layoutY="14.0"/>
        <Label layoutX="14.0" layoutY="44.0" text="Name:"/>
        <TextField fx:id="nameField" layoutX="100.0" layoutY="44.0"/>
        <Label layoutX="14.0" layoutY="74.0" text="Type:"/>
        <TextField fx:id="typeField" layoutX="100.0" layoutY="74.0"/>
        <Label layoutX="14.0" layoutY="104.0" text="Rarity:"/>
        <TextField fx:id="rarityField" layoutX="100.0" layoutY="104.0"/>
        <Label layoutX="14.0" layoutY="134.0" text="HP:"/>
        <TextField fx:id="hpField" layoutX="100.0" layoutY="134.0"/>
        <Label layoutX="14.0" layoutY="164.0" text="Picture URL:"/>
        <TextField fx:id="pictureUrlField" layoutX="100.0" layoutY="164.0"/>
        <Button layoutX="14.0" layoutY="200.0" mnemonicParsing="false" text="Save" onAction="#handleOk"/>
        <Button layoutX="100.0" layoutY="200.0" mnemonicParsing="false" text="Cancel" onAction="#handleCancel"/>
    </children>
</AnchorPane>
